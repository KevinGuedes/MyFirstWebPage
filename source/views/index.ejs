<!DOCTYPE html>
<html lang="en">

<head>
    <%- include("./partials/head") %>
</head>

<body>
    <div class="menu">
        <img src="/images/menu.jpg" alt="Math Symbols">
        <h1>Choose An Algorithm</h1>
    
        <div class="menu-items">
           <ol>
                <% menu.map(item => { %>
                    <br>
                    <li>
                        <a class="item" href="<%= item.href %>">
                            <%= item.method %>
                        </a>
                    </li>
                <% }) %>
           </ol>
        </div>
        <%- include("./partials/madeby") %>
    </div>
</body>

</html>